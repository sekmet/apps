{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "target": "es2017",
    "noImplicitAny": false,
    "rootDir": ".",
    "outDir": "lib",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false
